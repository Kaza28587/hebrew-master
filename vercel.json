{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/signup",
      "dest": "/public/signup.html"
    },
    {
      "src": "/signup.html",
      "dest": "/public/signup.html"
    },
    {
      "src": "/login",
      "dest": "/public/login.html"
    },
    {
      "src": "/login.html",
      "dest": "/public/login.html"
    },
    {
      "src": "/dashboard",
      "dest": "/public/dashboard.html"
    },
    {
      "src": "/dashboard.html",
      "dest": "/public/dashboard.html"
    },
    {
      "src": "/lesson",
      "dest": "/public/lesson.html"
    },
    {
      "src": "/lesson.html",
      "dest": "/public/lesson.html"
    },
    {
      "src": "/lesson2",
      "dest": "/public/lesson2.html"
    },
    {
      "src": "/lesson2.html",
      "dest": "/public/lesson2.html"
    },
    {
      "src": "/lesson3",
      "dest": "/public/lesson3.html"
    },
    {
      "src": "/lesson3.html",
      "dest": "/public/lesson3.html"
    },
    {
      "src": "/firebase-config.js",
      "dest": "/public/firebase-config.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "cleanUrls": false
}
